<html>
<center><h1>Current Members</h1></center>

<body>
  <center><a href='/members/new'>Add Member</a>  </center>
  <br>
  <% @members.each do |member| %>
  <table class="alt">
    <tr>
      <th>First Name:</th>
      <th>Last Name</th>
      <th>Membership:</th>
    </tr>
    <tr>
      <td><%= member.first_name %> </td>
      <td><%= member.last_name %></td>
      <td><%= member.membership %></td>
    </table>
    <br>
    <center><a href="/members/<%= member.id %>/edit">UPDATE</a> </center>
    <br>
    <center><a href="/members/<%= member.id %>/show">SHOW</a></center>
    <br>

    <% end %>
    <hr/>
  </body>
  </html>
